#encoding "utf-8"    // сообщаем парсеру о том, в какой кодировке написана грамматика
#GRAMMAR_ROOT S      // указываем корневой нетерминал грамматики

S -> Adj<gram='A', gnc-agr[1]>  Noun<gnc-agr[1], rt> ; // цепочка из прилагательного и существительного в нормализованной форме, прилагательное и существительное согласованы

S ->  Noun<gnc-agr[1], rt>  Adj<gnc-agr[1], gram='A'>  ; // цепочка из существительного и прилагательного в нормализованной форме, прилагательное и существительное согласованы


S -> Adv  Verb  ; // цепочка из наречия и глагола

S -> Verb  Adv  ; // цепочка из  глагола и наречия 

S -> Noun<gnc-agr[1]>  Noun<gnc-agr[1]> ; // цепочка из двух существительных, согласованных по роду, числу, падежу

S ->Noun<sp-agr[4]>  Verb<sp-agr[4]> ; // цепочка из нормализованного существительного и глагола, согласованного с ним

