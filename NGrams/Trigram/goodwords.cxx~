#encoding "utf-8"    // сообщаем парсеру о том, в какой кодировке написана грамматика
#GRAMMAR_ROOT S      // указываем корневой нетерминал грамматики

S -> Adj<gram='A', gnc-agr[1]> interp(NGrams.adj)  Noun<gnc-agr[1], rt>) interp (NGrams.noun::norm="nom,sg") ;

//S -> Adj<gram='A', gnc-agr[1]> interp(NGrams.adj)  Noun<gnc-agr[1], rt>) interp (NGrams.noun::norm="nom,sg") ; // цепочка из прилагательного и существительного в нормализованной форме, прилагательное и существительное согласованы

S ->  Noun<gnc-agr[1], rt> interp (NGrams.noun::norm="nom,sg") Adj<gnc-agr[1], gram='A'> interp(NGrams.adj) ; // цепочка из существительного и прилагательного в нормализованной форме, прилагательное и существительное согласованы


S -> Adv interp (NGrams.adv) Verb interp (NGrams.verb) ; // цепочка из наречия и глагола

S -> Verb interp (NGrams.verb) Adv interp (NGrams.adv) ; // цепочка из  глагола и наречия 

S -> Noun<gnc-agr[1]> interp (NGrams.noun::norm="nom,sg") Noun<gnc-agr[1]> interp (NGrams.noun::norm="nom,sg"); // цепочка из двух существительных, согласованных по роду, числу, падежу

S ->Noun<sp-agr[4]> interp (NGrams.noun::norm="nom,sg")  Verb<sp-agr[4]> interp (NGrams.verb) ; // цепочка из нормализованного существительного и глагола, согласованного с ним

